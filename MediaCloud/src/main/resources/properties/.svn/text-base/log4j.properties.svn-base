log4j.rootLogger=INFO,dayinfo
log4j.appender.errorinfo = org.apache.log4j.RollingFileAppender
log4j.appender.errorinfo.File = ${catalina.base}/logs/errorlog.log
log4j.appenile.errorinfo.Append = true
log4j.appender.errorinfo.MaxFileSize=1MB
log4j.appender.errorinfo.layout = org.apache.log4j.PatternLayout
log4j.appender.errorinfo.layout.ConversionPattern =%-d{yyyy-MM-dd HH\:mm\:ss,SSS} [%c]-[%p] %m%n
log4j.appender.errorinfo.Encoding=UTF-8
log4j.appender.dayinfo = org.apache.log4j.RollingFileAppender
log4j.appender.dayinfo.File = ${catalina.base}/logs/infolog.log
log4j.appenile.dayinfo.Append = true
log4j.appender.dayinfo.MaxFileSize=1MB
log4j.appender.dayinfo.layout = org.apache.log4j.PatternLayout
log4j.appender.dayinfo.layout.ConversionPattern =%-d{yyyy-MM-dd HH\:mm\:ss,SSS} [%c]-[%p] %m%n
log4j.appender.dayinfo.Encoding=UTF-8
